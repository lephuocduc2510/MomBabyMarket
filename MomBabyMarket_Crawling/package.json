{
  "name": "mombabymarket-crawler",
  "version": "1.0.0",
  "description": "Web crawler for MomBabyMarket case study using JavaScript and Docker",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "docker:build": "docker build -t mombabymarket-crawler .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f crawler",
    "crawl": "npm start"
  },
  "dependencies": {
    "puppeteer": "^21.5.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.1.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["crawler", "scraper", "facebook", "instagram", "docker", "javascript"],
  "author": "Hekate Team",
  "license": "MIT"
}
